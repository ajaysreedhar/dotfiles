{
  "icon_theme": {
    "mode": "dark",
    "light": "Zed (Default)",
    "dark": "Zed (Default)"
  },
  "auto_update": false,
  "autosave": "off",
  "base_keymap": "None",
  "buffer_font_family": "JetBrainsMonoNL Nerd Font",
  "buffer_font_size": 16,
  "buffer_line_height": {
    "custom": 1.5
  },
  "git_panel": {
    "button": true,
    "default_width": 480,
    "dock": "right",
    "fallback_branch_name": "main",
    "scrollbar": {
      "show": null
    },
    "status_style": "icon"
  },
  "languages": {
    "C": {
      "tab_size": 4
    },
    "CSS": {
      "tab_size": 2,
      "format_on_save": "on"
    },
    "Go": {
      "tab_size": 4
    },
    "JavaScript": {
      "tab_size": 2,
      "format_on_save": "off"
    },
    "JSON": {
      "tab_size": 2,
      "format_on_save": "on"
    },
    "JSONC": {
      "tab_size": 2,
      "format_on_save": "on"
    },
    "TypeScript": {
      "tab_size": 2,
      "format_on_save": "on"
    }
  },
  "preferred_line_length": 120,
  "show_whitespaces": "selection",
  "tab_size": 4,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": {
    "mode": "dark",
    "light": "Charcoal Default",
    "dark": "Charcoal Default"
  },
  "ui_font_size": 16
}
