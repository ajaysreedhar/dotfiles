{
  "auto_update": false,
  "autosave": "off",
  "base_keymap": "None",
  "buffer_font_size": 16,
  "buffer_font_family": "JetBrainsMonoNL Nerd Font",
  "buffer_font_features": {
    "calt": 0
  },
  "buffer_line_height": {
    "custom": 1.5
  },
  "disable_ai": true,
  "format_on_save": "on",
  "git_panel": {
    "button": true,
    "default_width": 480,
    "dock": "right",
    "fallback_branch_name": "main",
    "scrollbar": {
      "show": null
    },
    "status_style": "icon"
  },
  "icon_theme": "Zed (Default)",
  "languages": {
    "C": {
      "tab_size": 4
    },
    "CSS": {
      "tab_size": 2,
      "format_on_save": "on"
    },
    "Go": {
      "tab_size": 4
    },
    "JavaScript": {
      "tab_size": 2,
      "format_on_save": "off"
    },
    "JSON": {
      "tab_size": 2,
      "format_on_save": "on"
    },
    "JSONC": {
      "tab_size": 2,
      "format_on_save": "on"
    },
    "TypeScript": {
      "tab_size": 2,
      "format_on_save": "on"
    }
  },
  "minimap": {
    "show": "auto"
  },
  "preferred_line_length": 120,
  "show_whitespaces": "selection",
  "tab_size": 4,
  "terminal": {
    "font_family": "JetBrainsMonoNL Nerd Font",
    "font_size": 16,
    "line_height": "standard"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Charcoal Default"
  },
  "ui_font_size": 16
}
